<body>
	<script type="text/javascript">
	for (var i = 1; i < 10; i++) {
		for (var j = 2; j < 10; j++) {
			var a= i*j;
			if(a < = 9) {
			document.writeln(j+"*"+i+"&nbsp;"+j*i+"l");
			}
			else {
				document.writeln(j+"*"+i+"="+j*i+"l");
				}
			}
			document.write("<br>");
			}
	</script>
</body>
